{
  "srcPaths": {
    "make": "src/project.make",
    "drupal": "src"
  },
  "domain": "project.vm",
  "packages": {
    "srcFiles": [
      "!sites/*/files/**",
      "!xmlrpc.php",
      "!modules/php/*"
    ],
    "projFiles": [
      "README*",
      "bin/**"
    ]
  },
  "phpcs": {
    "path": "vendor/bin/phpcs"
  },
  "phpmd": {
    "path": "vendor/bin/phpmd"
  },
  "drush": {
    "cmd": "vendor/bin/drush"
  },
  "behat": {
    "flags": "--tags ~@wip"
  },
  "eslint": true,
  "buildPaths": {
    "html": "/var/www/html"
  },
  "generated": {
    "modified": [
      {
        "name": "generator-p2-env",
        "version": "0.1.0"
      }
    ]
  }
}
